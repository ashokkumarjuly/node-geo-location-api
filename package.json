        "husky": "^7.0.4",
        "inquirer-directory": "^2.2.0",
        "jest": "^27.5.1",
        "jest-serial-runner": "^1.2.0",
        "jest-stare": "^2.3.0",
        "lint-staged": "^12.3.3",
        "plop": "^3.0.5",
        "prettier": "^2.5.1",
        "pretty-error": "^4.0.0",
        "sonarqube-scanner": "^2.8.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.5"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts}": [
            "prettier --write",
            "eslint ./src/**/*.{ts,tsx,jsx} --fix",
            "git add"
        ],
        "*.+(js|json|md)": [
            "prettier --write"
        ]
    }
}
