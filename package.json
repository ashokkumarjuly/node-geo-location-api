        "memorystore": "^1.6.7",
        "method-override": "^3.0.0",
        "moment": "^2.29.1",
        "mongoose": "^6.2.4",
        "morgan": "^1.10.0",
        "mysql2": "^2.3.3",
        "nodemailer": "^6.7.2",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pm2": "^5.1.2",
        "pug": "^3.0.2",
        "rate-limiter-flexible": "^2.3.6",
        "redis": "^4.0.4",
        "sequelize": "^6.16.1",
        "short-unique-id": "^4.4.4",
        "source-map-support": "^0.5.21",
        "swagger-ui-express": "^4.3.0",
        "tsc-watch": "^4.6.0",
