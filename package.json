{
    "name": "geo-places-api-server",
    "version": "1.0.0",
    "description": "Restfull API build usng express and typescript",
    "engines": {
        "node": ">=14",
        "npm": ">=6.9.0"
    },
    "main": "./src/server.js",
    "scripts": {
        "start": "cross-env NODE_ENV=production node ./src/server.js",
        "build": "cross-env NODE_ENV=production gulp",
        "local": "cross-env NODE_ENV=local ts-node-dev  --respawn --transpile-only --watch \"src/**\" --ignore-watch=\"['src/**/*.json']\" ./src/server.ts ",
        "dev": "cross-env NODE_ENV=development ts-node-dev  --respawn --transpile-only --watch \"src/**\" --ignore-watch=\"['src/**/*.json']\" ./src/server.ts ",
        "test": "npm run test:all",
        "test:all": "npm run test:unit && npm run test:integration",
        "test:unit": "cross-env NODE_ENV=local DB_LOG_QUERY=false ENABLE_DEBUG=false BLOCKCHAIN_ENABLED=false node --trace-warnings ./node_modules/jest/bin/jest --detectOpenHandles --runInBand --no-coverage",
        "test:unit:watch": "cross-env NODE_ENV=testing DB_LOG_QUERY=false BLOCKCHAIN_ENABLED=false  node --trace-warnings ./node_modules/jest/bin/jest --detectOpenHandles --onlyChanged --runInBand --no-coverage --colors --watch",
        "test:integration": "cross-env NODE_ENV=testing jest --runInBand --testTimeout=10000 -c jest.integration.config.js --detectOpenHandles --no-coverage --verbose -i",
        "coverage": "cross-env NODE_ENV=testing DB_LOG_QUERY=false ENABLE_DEBUG=false  jest --coverage --verbose --reporters default jest-stare",
        "compileTs": "gulp compileTs",
        "lint": "eslint ./src/**/*.{ts,tsx,jsx}",
        "lint:fix": "eslint ./src/**/*.{ts,tsx,jsx} --fix",
        "prettier:check": "prettier --check src/**/*.{ts,tsx,jsx}",
        "prettier:write": "prettier --write src/**/*.{ts,tsx,jsx}",
        "doc": "apidoc -i src -o docs/html && apidoc-markdown -p docs/html -o docs/DOCS.md",
        "sonar-analyse": "cross-env NODE_ENV=development ts-node-dev sonar-scanner.ts",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "clean": "rm -rf ./dist coverage",
        "gt": "plop",
        "stop": "pm2 stop all"
    },
    "author": "Impiger",
    "license": "ISC",
    "dependencies": {
        "@joi/date": "^2.1.0",
        "@sendgrid/helpers": "^7.6.0",
        "@sendgrid/mail": "^7.6.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/express-session": "^1.17.4",
        "@types/ua-parser-js": "^0.7.36",
        "axios": "^0.25.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.1",
